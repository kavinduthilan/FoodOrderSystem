{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "FoodStoreDatabase": {
    "ConnectionString": "mongodb://localhost:27017/",
    "DatabaseName": "foodStore",
    "UserCollectionName": "user",
    "FoodCollectionName": "food"
  },
  "Jwt": {
    "Key": "YourSuperLongSecretKeyMustBeAtLeast32Chars",
    "Issuer": "JwtIssuer",
    "Audience": "JwtAudience",
    "Subject": "JwtSubject"
  }
}